{
    "name": "draggable-panel",
    "version": "1.0.5",
    "description": "Vue draggable panel",
    "author": "Cainier <xingrong.dev@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/Cainier/draggable-panel.git"
    },
    "license": "MIT",
    "keywords": [
        "vue",
        "vue3",
        "drag",
        "resize",
        "draggable",
        "scale",
        "zoom",
        "component",
        "draggable-panel"
    ],
    "main": "./dist/draggable-panel.umd.js",
    "module": "./dist/draggable-panel.es.js",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/draggable-panel.es.js",
            "require": "./dist/draggable-panel.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "private": false,
    "scripts": {
        "dev": "vite",
        "build:lib": "vite build --mode lib",
        "build:example": "vite build --mode example",
        "build:release": "sh ./release.sh"
    },
    "dependencies": {
        "sass": "^1.52.2",
        "typescript": "^4.7.3",
        "vue": "^3.2.25"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^2.3.3",
        "vite": "^2.9.9"
    }
}
